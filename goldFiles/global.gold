
#include <assert.h>
#include <cstring>
 
using namespace std;
 
// Global variables defining starting state
unsigned int b = 0;
 
void f_forward();
void f_reverse();
 
 
void f_forward()
{
    unsigned int a = b;
    a += 2;
    
}
 
void f_reverse()
{
    unsigned int a = (b) + (2);
    a -= 2;
    
}
 
int main()
{
    
    f_forward();
}
