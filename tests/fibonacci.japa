procedure fib(int x1, int x2, int n)
{
    if (n == 0)
    {
        x1 += 1
        x2 += 1
    } fi (x1 == x2)
    else
    {
        n -= 1
        call fib(x1, x2, n)
        x1 += x2
        x1 <=> x2
    }
}

procedure main()
{
    int x1
    int x2
    local int n = 4
    call fib(x1, x2, n)
    dealloc int n = 0
}
